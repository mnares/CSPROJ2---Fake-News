{
  "manifest_version": 2,
  "name": "Fakenews",
  "description":"This extension identifies fake news in facebook",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
    "permissions": [
   "tabs",
   "unlimitedStorage"
   ],
  "icons":{
    "64" : "proof.png"
    },
  "browser_action": {
  "default_icon": "proof.png",
  "default_popup": "popup.html"
}

}